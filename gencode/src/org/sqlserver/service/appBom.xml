<properties>
	
		<property name="addAppBom">
		<![CDATA[
			insert into TD_APP_BOM (ID,APP_NAME,APP_NAME_EN,APPLY_DOMAIN,BM,DB_TYPE,DEPARTMENT_DEVELOP,DEPARTMENT_MAINTAIN,DESCRIPTION,DEVELOP_TOOL,DOMAIN_URL,EVOLVE_DEPART,EVOLVE_PLAN,EVOLVE_STRATEGY,MAIN_DESCRIPTION,MANAGE_SCOPE,PLAN_TYPE,PRODUCT_MANAGER,RD_TYPE,SOFT_LANGUAGE,SOFT_LEVEL,START_YEAR,STATE,STRUCT_MODE,SUPPLIER,SYS_MANAGER,VERSION_NO) values(#[id],#[appName],#[appNameEn],#[applyDomain],#[bm],#[dbType],#[departmentDevelop],#[departmentMaintain],#[description],#[developTool],#[domainUrl],#[evolveDepart],#[evolvePlan],#[evolveStrategy],#[mainDescription],#[manageScope],#[planType],#[productManager],#[rdType],#[softLanguage],#[softLevel],#[startYear],#[state],#[structMode],#[supplier],#[sysManager],#[versionNo])
		]]>
	</property>
		<property name="deleteByKey">
		<![CDATA[
			delete from TD_APP_BOM where ID=?
		]]>
	</property>
		<property name="updateAppBom">
		<![CDATA[
			update TD_APP_BOM set ID=#[id],APP_NAME=#[appName],APP_NAME_EN=#[appNameEn],APPLY_DOMAIN=#[applyDomain],BM=#[bm],DB_TYPE=#[dbType],DEPARTMENT_DEVELOP=#[departmentDevelop],DEPARTMENT_MAINTAIN=#[departmentMaintain],DESCRIPTION=#[description],DEVELOP_TOOL=#[developTool],DOMAIN_URL=#[domainUrl],EVOLVE_DEPART=#[evolveDepart],EVOLVE_PLAN=#[evolvePlan],EVOLVE_STRATEGY=#[evolveStrategy],MAIN_DESCRIPTION=#[mainDescription],MANAGE_SCOPE=#[manageScope],PLAN_TYPE=#[planType],PRODUCT_MANAGER=#[productManager],RD_TYPE=#[rdType],SOFT_LANGUAGE=#[softLanguage],SOFT_LEVEL=#[softLevel],START_YEAR=#[startYear],STATE=#[state],STRUCT_MODE=#[structMode],SUPPLIER=#[supplier],SYS_MANAGER=#[sysManager],VERSION_NO=#[versionNo] where ID=#[id]
		]]>
	</property>
		<property name="selectById">
		<![CDATA[
			select * from TD_APP_BOM where ID=?
		]]>
	</property>
		<property name="queryListAppBom">
		<![CDATA[
			select * from TD_APP_BOM where 1=1
						#if($bm && !$bm.equals(""))
				and BM like #[bm]		
			#end
			#if($sortKey && !$sortKey.equals(""))
		order by $sortKey #if($sortDesc)desc#else asc#end
	#end

		]]>
	</property>
		<property name="queryPagineAppBom">
		<![CDATA[
			select * from TD_APP_BOM where 1=1
						#if($bm && !$bm.equals(""))
				and BM like #[bm]		
			#end
			
		]]>
	</property>
		<property name="AppBomOrderby">
		<![CDATA[
			#if($sortKey && !$sortKey.equals(""))
		order by $sortKey #if($sortDesc)desc#else asc#end
	#end

		]]>
	</property>
	</properties>