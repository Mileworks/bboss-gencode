<properties>
	
		<property name="addFgwXmsbk">
		<![CDATA[
			insert into ta_fgw_xmsbk (SBXMID,BNDSCZTZ,BW,BX,BZ,CSDW,DBXM,DBXMID,DJ,GSZTZ,HYID,ITEMNAME,JJID,JSDZ,JSGM,JSND,JSXZID,KSND,LSGXID,LXRJFS,QQGZJZ,QQGZRW,REGION_ID,REGION_NAME,SBDW,SBDWID,SBND,SBR,SBRLXFS,SBRQ,SFBXG,SFSBJ,SFYJBYY,SSHJ,TBDWID,TBSJ,TBYH,TJJDID,TZLX,XMBH,XMDW,XMGKDW,XMLBID,XMXZ,YQID,ZT,ZYJSNR) values(#[sbxmid],#[bndscztz],#[bw],#[bx],#[bz],#[csdw],#[dbxm],#[dbxmid],#[dj],#[gsztz],#[hyid],#[itemname],#[jjid],#[jsdz],#[jsgm],#[jsnd],#[jsxzid],#[ksnd],#[lsgxid],#[lxrjfs],#[qqgzjz],#[qqgzrw],#[regionId],#[regionName],#[sbdw],#[sbdwid],#[sbnd],#[sbr],#[sbrlxfs],#[sbrq],#[sfbxg],#[sfsbj],#[sfyjbyy],#[sshj],#[tbdwid],#[tbsj],#[tbyh],#[tjjdid],#[tzlx],#[xmbh],#[xmdw],#[xmgkdw],#[xmlbid],#[xmxz],#[yqid],#[zt],#[zyjsnr])
		]]>
	</property>
		<property name="deleteByKey">
		<![CDATA[
			delete from ta_fgw_xmsbk where SBXMID=?
		]]>
	</property>
		<property name="updateFgwXmsbk">
		<![CDATA[
			update ta_fgw_xmsbk set SBXMID=#[sbxmid],BNDSCZTZ=#[bndscztz],BW=#[bw],BX=#[bx],BZ=#[bz],CSDW=#[csdw],DBXM=#[dbxm],DBXMID=#[dbxmid],DJ=#[dj],GSZTZ=#[gsztz],HYID=#[hyid],ITEMNAME=#[itemname],JJID=#[jjid],JSDZ=#[jsdz],JSGM=#[jsgm],JSND=#[jsnd],JSXZID=#[jsxzid],KSND=#[ksnd],LSGXID=#[lsgxid],LXRJFS=#[lxrjfs],QQGZJZ=#[qqgzjz],QQGZRW=#[qqgzrw],REGION_ID=#[regionId],REGION_NAME=#[regionName],SBDW=#[sbdw],SBDWID=#[sbdwid],SBND=#[sbnd],SBR=#[sbr],SBRLXFS=#[sbrlxfs],SBRQ=#[sbrq],SFBXG=#[sfbxg],SFSBJ=#[sfsbj],SFYJBYY=#[sfyjbyy],SSHJ=#[sshj],TBDWID=#[tbdwid],TBSJ=#[tbsj],TBYH=#[tbyh],TJJDID=#[tjjdid],TZLX=#[tzlx],XMBH=#[xmbh],XMDW=#[xmdw],XMGKDW=#[xmgkdw],XMLBID=#[xmlbid],XMXZ=#[xmxz],YQID=#[yqid],ZT=#[zt],ZYJSNR=#[zyjsnr] where SBXMID=#[sbxmid]
		]]>
	</property>
		<property name="selectById">
		<![CDATA[
			select * from ta_fgw_xmsbk where SBXMID=?
		]]>
	</property>
		<property name="queryListFgwXmsbk">
		<![CDATA[
			select * from ta_fgw_xmsbk where 1=1
#if($xmgkdw && !$xmgkdw.equals(""))
	and xmgkdw like #[xmgkdw]		
#end
#if($jsdz && !$jsdz.equals(""))
	and jsdz like #[jsdz]		
#end
#if($sortKey && !$sortKey.equals(""))
order by $sortKey #if($sortDesc)desc#else asc#end
#if(!$sortKey.equals("jjid"))
	,jjid desc#end	
#else
order by jjid desc
#end
		]]>
	</property>
	</properties>